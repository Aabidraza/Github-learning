	/********** Multiple Value ******************/
	function multi($n){

		if($n==0){
			return 1;
		}else{
			return $n * multi($n-1);
		}
	}

	echo "Multiplicatio : " .multi(3);echo "<br>";echo "<br>";
	/*****************************************/
	/*********** Print Value *****************/
	function num($num){
		echo $num;echo "<br>";
		if($num==5){
			return 1;
		}else{
			return num($num+1);
		}
	}
	echo  "Print Value : " ; echo "<br>";num(1);
	/*****************************************/
	

?>
